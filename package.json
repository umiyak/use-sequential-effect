{
  "name": "@umiyak/use-sequential-effect",
  "version": "0.1.0-beta.0",
  "description": "A React hook that runs useEffect callbacks sequentially",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run",
    "build": "tsc && vite build",
    "coverage": "vitest --run --coverage",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.66",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "jsdom": "^26.1.0",
    "lefthook": "^1.12.3",
    "react": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.3",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "react",
    "typescript",
    "component",
    "library"
  ],
  "author": "umiyak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/umiyak/use-sequential-effect"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2"
}
